(function(){
  const domContentLoadedCB = () => {
    $('#add-criterion-level').click((e) => {
      add_level("rubric_form");
      e.preventDefault();
    })
  }

  if (document.readyState === "loading") {
    document.addEventListener("DOMContentLoaded", domContentLoadedCB);
  }else{
    domContentLoadedCB()
  }
})();
